<h1>{COMM_HEADER}</h1>
<p>{COMM_EXPLAIN}</p>
<table width="100%" cellpadding="3" cellspacing="1" border="0" class="forumline">
	<tr>
		<th class="thTop" nowrap="nowrap" width="" colspan="2">{USER}</th>
		<td class="row2" width="5%" rowspan="{ROWSPAN_VAL}"></td>
		<th class="thTop" nowrap="nowrap" width="" colspan="2">{COM_COMMISSION_PAID}</th>
		<td class="row2" width="5%" rowspan="{ROWSPAN_VAL}"></td>
		<th class="thTop" nowrap="nowrap" width="" colspan="2">{COM_COMMISSION_PENDING}</th>
	<tr>
		<td class="catLeft" align="center"><span class="cattitle">#</span></td>
		<td class="catLeft" align="center"><span class="cattitle">{COM_NAME}</span></td>
		<td class="catLeft" align="center"><span class="cattitle">{COM_NUM_BETS}</span></td>
		<td class="catLeft" align="center"><span class="cattitle">{COM_COMMISSION_PAID}</span></td>
		<td class="catLeft" align="center"><span class="cattitle">{COM_NUM_BETS}</span></td>
		<td class="catLeft" align="center"><span class="cattitle">{COM_COMMISSION_DUE}</span></td>
	</tr>
	<!-- BEGIN commission -->
	<tr> 
		<td class="{commission.ROW_CLASS}" align="center" valign="middle"><span class="gen"><b>{commission.POS}</b></span></td>
		<td class="{commission.ROW_CLASS}" align="left" valign="middle"><span class="gen"><b>{commission.NAME}</b></span></td>
		<td class="{commission.ROW_CLASS}" align="center" valign="middle"><span class="gen"><b>{commission.NUM_BETS}</b></span></td>
		<td class="{commission.ROW_CLASS}" align="center" valign="middle"><span class="gen"><b>{commission.COMMISSION}</b></span></td>
		<td class="{commission.ROW_CLASS}" align="center" valign="middle"><span class="gen">{commission.UN_NUM_BETS}</span></td>
		<td class="{commission.ROW_CLASS}" align="center" valign="middle"><span class="gen">{commission.UN_COMMISSION}</span></td>
	<!-- END commission -->
</table>
